{
  "name": "didbot/didbot-api",
  "description": "API for didbot.com",
  "homepage": "https://github.com/didbot/didbot-api",
  "authors": [
    {
      "name": "Ben Bjurstrom",
      "email": "ben@jelled.com"
    }
  ],
  "keywords": [
    "didbot",
    "api",
    "task",
    "tracker",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "laravel/passport": "^2.0",
    "spatie/laravel-fractal": "^3.0",
    "webpatser/laravel-uuid": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "orchestra/testbench": "~3.4.0",
    "orchestra/testbench-browser-kit": "^3.4",
    "mockery/mockery": "^0.9.8"
  },
  "autoload": {
    "psr-4": {
      "Didbot\\DidbotApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Didbot\\DidbotApi\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
